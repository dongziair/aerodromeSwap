# Base 链 RPC 节点
RPC_URL=https://mainnet.base.org

# 钱包私钥，逗号分隔（不要加 0x 前缀也可以，脚本会自动处理）
PRIVATE_KEYS=your_private_key_1,your_private_key_2,your_private_key_3

# SOCKS5 代理列表，逗号分隔，格式：ip:port:user:pass
PROXIES=ip1:port1:user1:pass1,ip2:port2:user2:pass2,ip3:port3:user3:pass3

# gas 价格上限（gwei），超过此值暂停交互
MAX_GAS_GWEI=0.05

# 每日 swap 交互次数范围
MIN_DAILY_SWAPS=2
MAX_DAILY_SWAPS=8

# 每日自转 ETH 次数范围
MIN_DAILY_SELF_TRANSFERS=50
MAX_DAILY_SELF_TRANSFERS=90

# 自转 ETH 金额占余额百分比范围
SELF_TRANSFER_MIN_PERCENT=1
SELF_TRANSFER_MAX_PERCENT=5
